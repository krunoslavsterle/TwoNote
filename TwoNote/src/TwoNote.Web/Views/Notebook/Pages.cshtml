@{
    ViewData["Title"] = "Pages";
}

@model IEnumerable<TwoNote.Core.Entities.PageEntity>

<div>
    <h1>List of pages:</h1>

    <ul>
        @foreach (var notebook in Model)
        {
            <li>
                <a asp-area="" asp-controller="Notebook" asp-action="Pages">@notebook.Name</a>
            </li>
        }
    </ul>


</div>